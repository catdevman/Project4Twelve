{
    "build": {
        "options": {
            "presets": [ "es2015" ],
            "plugins": [ "transform-es2015-modules-amd" ],
            "comments": false
        },
        "files": [ {
            "expand": true,
            "cwd": "www/js/",
            "src": [ "**/*.js", "!require-config.js" ],
            "dest": "build/convert/"
        } ]
    }
}
